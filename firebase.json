{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html",
        "pre-deploy": "yarn build"
      }
    ]
  },
  "remoteconfig": {
    "template": "firebase/remoteconfig.template.json"
  }
}
